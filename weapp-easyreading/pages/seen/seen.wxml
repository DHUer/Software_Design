<scroll-view scroll-y="true" class="container" >

    <block wx:for="{{seenList}}" wx:for-index="idx" wx:for-item="item"  >

       <view class="feed-item" bindtap="bindItemTap" data-idx="{{item.id}}" data-type="{{item.btype}}">

            <view class="feed-content">

                <view class="question" >
                    <a class="question-link">
                        <text>{{item.title}}</text>
                    </a>
                </view>


<view class="art-more">

<view class="pic">
<view class="left">
              <view class="art-tag">
                <text class="tag">{{item.btype}}</text>
                <text class="tag">{{item.type}}</text>
               </view>
               </view>

<view class="right">
                    <view class="book">
                        <image src="../../images/book.png"></image>
                        <text>{{item.num}}ËØç </text>
                    </view>
                    <view class="watch">
                        <image src="../../images/eye.png"></image>
                        <text>3316</text>
                    </view>
                    <view class="comment">
                        <image src="../../images/star.png"></image>
                        <text>27</text>
                    </view>

</view>
</view>
            </view>

            </view>

        </view>

    </block>
</scroll-view>
